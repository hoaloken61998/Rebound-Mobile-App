    <string name="faq_delivery_time">Thời gian giao hàng ước tính là bao lâu?</string>
    <string name="faq_delivery_time_answer">Giao hàng mất 2–3 ngày làm việc tùy theo địa điểm.</string>

    <string name="faq_delivery_change_address">Tôi có thể đổi địa chỉ giao hàng sau khi đặt không?</string>
